	{	
		case 1:{
				cout<<"Enter the size of sequence no."<<endl;cin>>size;
				int sent_window[(int)pow(2,size)-1]; 
				break;
		       }
		case 2:{
				break;
		       }
		default:{
				cout<<"Sorry! You have entered a wrong option."<<endl;break;
			}
	}
	return 0;
}
